package page

templ Err500(text string) {
<div role="alert" class="alert alert-error">
    <div>
        <h3 class="text-3xl">Whoops! INTERNAL SERVER ERROR</h3>
        <div class="p-5"><code class="font-mono">{ text }</code></div>
        <img src="/assets/err_500.png" alt="500" width="500"/>
    </div>
</div>
}
