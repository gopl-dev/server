package component


// https://heroicons.com/solid

var icons = map[string]templ.Component {
	"bug": IconBug(),
	"user": IconUser(),
	"envelope": IconEnvelope(),
	"sign-in": IconSignIn(),
	"sign-up": IconSignUp(),
}

templ Icon(name string) {
	if _, ok := icons[name]; !ok {
    name = "bug"
  }

	@icons[name]
}

